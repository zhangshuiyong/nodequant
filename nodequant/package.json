{
  "name": "nodequant",
  "version": "2.0.0",
  "scripts": {
    "start": "node ./bin/www"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "eqeqeq": [
        "error"
      ],
      "curly": [
        "error"
      ]
    }
  },
  "dependencies": {
    "async": "^2.5.0",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "influx": "^5.0.7",
    "morgan": "~1.9.1",
    "redis": "^2.7.1",
    "serve-favicon": "~2.4.2"
  }
}
